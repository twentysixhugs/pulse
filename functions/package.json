    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^1.7.2",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.1",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.2.0",
    "typescript": "^5.4.5"
  },
  "private": true
